/****************************************************** Common Style ****************************************************/

body{
	font-family: Roboto Slab;
	color: #4c4c4c;
	font-size: 15px;
}
a,
a:focus,
a:hover{
	text-decoration: underline;
	color: #08b4e0;
  transition: ease-in-out all 0.4s;
  -webkit-transition: ease-in-out all 0.4s;
  -ms-transition: ease-in-out all 0.4s;
  -o-transition: ease-in-out all 0.4s;
}
h1, h2, h3, h4, h5, h6{
	font-family: Lato;
	font-weight: 900;
  text-transform: uppercase;
}
h1{ font-size: 36px; }
h2{ font-size: 30px; }
h3{ font-size: 24px; }
h4{ font-size: 20px; }
h5{ font-size: 16px; }
h6{ font-size: 15px; }
.no-padding{ padding: 0 !important; }
.padding-70{ padding: 70px 0; }
.padding-top-70{ padding: 70px 0 30px 0; }
.light-grey-bg{ background-color:#f2f2f2; }
.light-creame-bg{ background-color:#f7f7f7; }
button{ box-shadow: none; border: none; }
.button.small {
  border-radius: 3px;
  color: #4c4c4c;
  font-family: Lato;
  font-size: 12px;
  font-weight: 900;
  padding: 5px 12px;
  text-decoration: none;
  text-transform: uppercase;
  display: inline-block;
}
.button.medium {
  border-radius: 3px;
  color: #4c4c4c;
  font-family: Lato;
  font-size: 13px;
  font-weight: 900;
  padding: 15px 30px;
  text-decoration: none;
  text-transform: uppercase;
  display: inline-block;
}
.button.large {
  border-radius: 3px;
  color: #4c4c4c;
  font-family: Lato;
  font-size: 14px;
  font-weight: 900;
  padding: 15px 30px;
  text-decoration: none;
  text-transform: uppercase;
  display: inline-block;
}
.button.border{ 
	border: 1px solid #cccccc; 
	background: transparent; 
  box-shadow:#fff 0 0 0 inset;
  transition: ease-in-out all 0.4s;
  -webkit-transition: ease-in-out all 0.4s;
  -ms-transition: ease-in-out all 0.4s;
  -o-transition: ease-in-out all 0.4s;
}
.button.border:hover{
  box-shadow: #fff 300px 0 0 inset;
  color: #4c4c4c;
}
.button.blue{ 
	background: #08b4e0; 
	color: #fff;
  box-shadow:transparent 0 0 0 inset;
  transition: ease-in-out all 0.4s;
  -webkit-transition: ease-in-out all 0.4s;
  -ms-transition: ease-in-out all 0.4s;
  -o-transition: ease-in-out all 0.4s;
}
.button.blue:hover{
  box-shadow:#f64900 300px 0 0 inset; 
}
input:focus,
textarea:focus{
  box-shadow: none !important;
  border-color: #9f9f9f !important;
}

