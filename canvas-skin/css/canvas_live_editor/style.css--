/*==========  Stem  ==========*/
.stem-wrapper {
  position: fixed;
  top: 0px;
  bottom: 100px;
  z-index: -99;
  width: 20px;
  left: 50%;
  transform: translate(-50%, 0);
}
.stem-wrapper.color-white .stem-background {
  background: #FFF;
}
.stem-wrapper .stem, .stem-wrapper .stem-background {
  position: fixed;
  top: 0px;
  width: 30px;
  margin: 0 auto;
}
.stem-wrapper .stem {
  background: #f2f2f2;
  height: 100%;
}
.stem-wrapper .stem-background {
  background: #08b4e0;
  height: 40%;
  -webkit-transition-duration: 0.5s;
  transition-duration: 0.5s;
}
.promises-wrapper {
  width: 100%;
}
