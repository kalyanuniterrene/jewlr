  font-size: 15px;
  text-transform: uppercase;
  font-family: Lato;
  font-weight: 900;
}
.price-size-table .sub-title .size-title {
    padding: 0 40px 0 30px;
}
.price-size-table ul li input[type=radio]{
  position: absolute;
  visibility: hidden;
}
.price-size-table ul {
    background: #f7f7f7 none repeat scroll 0 0;
    border: 1px solid #e6e6e6;
    box-shadow: 0 0 3px rgba(0, 0, 0, 0.2);
    width: 410px;
    box-sizing: border-box;
    float: right;
}
.price-size-table .price { font-weight: bold; }
.price-size-table .price strike {
    padding: 0 15px 0 0;
    font-weight: normal;
}
.price-size-table ul li label {
  cursor: pointer;
  display: inline-block;
  font-size: 15px;
  height: 30px;
  line-height: 30px;
  margin: 0 auto;
  padding: 0 0 0 65px;
  position: relative;
  transition: all 0.25s linear 0s;
  z-index: 9;
  font-weight: normal;
  width: 135px;
}
.price-size-table ul li.custom-size{ background: #f2f2f2; }
.price-size-table ul li.custom-size label{ width: auto; }
.price-size-table ul li.custom-size select{
  -moz-appearance:none;
  -webkit-appearance:none;
  -ms-appearance:none;
  -o-appearance:none;
  background: #fff url("../images/select-drop.png") no-repeat scroll 80% center;
  border: 1px solid #e6e6e6;
  border-radius: 3px;
  margin: 0 10px;
  padding: 9px;
  text-align: left;
  width: 60px;
}
.price-size-table ul li.custom-size .check{ top: 21px; }
.price-size-table ul li.final-price {
    color: #08b4e0;
    font-family: Lato;
    font-size: 24px;
    font-weight: 900;
    line-height: normal;
    padding: 14px 25px !important;
    vertical-align: middle;
}
.price-size-table ul li.final-price  strike {
    color: #808080;
    display: inherit;
    font-size: 14px;
    line-height: normal;
    margin: 0 0 -6px;
}
.price-size-table ul li.final-price .button {
    display: inline-block;
    margin: -12px 0 0 35px;
    vertical-align: middle;
}
.price-size-table ul li .check{
  border-radius: 100%;
    display: block;
    height: 20px;
    left: 20px;
    position: absolute;
    top: 16px;
    transition: border 0.25s linear 0s;
    width: 20px;
    border: 1px solid;
    z-index: 5;
}
.price-size-table ul li:hover .check {
  border: 1px solid #707070;
}
.price-size-table ul li .check::before {
    border-radius: 100%;
    content: "";
    display: block;
    height: 12px;
    left: 3px;
    margin: auto;
    position: absolute;
    top: 3px;
    transition: background 0.25s linear 0s;
    -webkit-transition: background 0.25s linear 0s;
    width: 12px;
}
.price-size-table input[type=radio]:checked ~ .check {
  border: 1px solid #08b4e0;
}
.price-size-table input[type=radio]:checked ~ .check::before{
  background: #08b4e0;
}
.price-size-table input[type=radio]:checked ~ label{
  color: #4c4c4c;
}



/********************************************************** Wall Display *****************************************************/
.panel-display{ padding: 70px 0; margin-bottom: -4px; }
.panel-display p{ font-size: 18px; }
.panel-display .filters {
  margin: 40px 0;
}
.panel-display .nav.nav-tabs{ border: none; margin: 35px 0; }
.panel-display .filter-button {
  color: #4c4c4c;
  background: transparent;
  padding: 0 8px;
  border: none; 
  text-decoration: underline;
}
.panel-display .filter-button:focus{
  outline: none; 
}
.panel-display .filter-button:hover{
  text-decoration: underline;
}
.panel-display .filter-button.active {
  font-weight: bold;
  color: #08b4e0;
}
.panel-display .panel-display-box {
  box-sizing: border-box;
  float: left;
  padding: 0 15px;
  width: 20%;
  min-height: 220px;
  margin-bottom: 30px;
}
.panel-display .panel-display-box .image-box {
  background: #e6e6e6 none repeat scroll 0 0;
  border-radius: 3px;
  display: table;
  height: 172px;
  width: 100%;
  position: relative;
}
.panel-display .tab-content{ margin-bottom: 25px; display: inline-block; }
.panel-display .panel-display-box .image-box .image {
  display: table-cell;
  vertical-align: middle;
  background: rgba(0, 0, 0, 0) none repeat scroll 0 0;
  transition:ease-in-out all 0.4s;
  -webkit-transition:ease-in-out all 0.4s;
  -ms-transition:ease-in-out all 0.4s;
  -o-transition:ease-in-out all 0.4s;
  border-radius: 3px;
}
.panel-display .panel-display-box .image-box .select-panel {
  left: 50%;
  position: absolute;
  top: 50%;
  transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  -ms-transform: translate(-50%, -50%);
  -o-transform: translate(-50%, -50%);
  width: 100%;
  opacity: 0;
  transition:ease-in-out all 0.4s;
  -webkit-transition:ease-in-out all 0.4s;
  -ms-transition:ease-in-out all 0.4s;
  -o-transition:ease-in-out all 0.4s;
}
.panel-display .panel-display-box .image-box .select-panel .button.medium {
  padding: 12px 25px;
  margin: 0;
}
.panel-display .panel-display-box .image-box:hover{
  box-shadow: 0 0 8px rgba(0, 0, 0, 0.3);
}
.panel-display .panel-display-box .image-box:hover .select-panel{
  opacity: 1;
}
.panel-display .panel-display-box .image-box:hover .image{
  background: rgba(0, 0, 0, 0.3) none repeat scroll 0 0;
}
.panel-display .panel-display-box .detail {
  font-size: 14px;
  padding-top: 5px;
}
.panel-display .panel-display-box .detail .price {
  color: #08b4e0;
  display: inline-block;
  font-weight: bold;
  width: 100%;
  margin-top: 5px;
}
.panel-display .panel-display-box .detail .price strike{
  font-weight: normal;
  color: #808080;
}
.panel-display .button{
  margin-top: 30px;
}



