	/* *** 13.12.2017 *** */



#flickerform{
	padding: 15px;
    margin: 15px 0;
    background: #f5f6fA;
    border-radius: 4px;
}

.form_wrapper {
	margin: 10px 0
}
.form_wrapper label{
	min-width: 70px;
}

.form_wrapper input, .form_wrapper select{
	position: relative ;
display: inline-block ;
-webkit-box-flex: 1;
-ms-flex: 1 ;
flex: 1 ;
-ms-flex-item-align: start ;
align-self: flex-start ;
padding: 10px 15px ;
margin-right: 15px ;
background: #fff ;
border: 1px solid #dde4f6 ;
border-radius: 4px ;
color: #08b4e0  ;
font-weight: bold ;
white-space: nowrap ;
text-overflow: ellipsis ;
overflow: hidden ;
-webkit-transition: all .2s ease-out ;
transition: all .2s ease-out ;
width: 82% ;
box-shadow: none ;
}
.form_wrapper span{
	font-size: 12px;
	float: right;
	margin-right: 25px;
}

#flickerform .form_wrapper .search {
	display: inline-block ;
background: #08b4e0  !important;
padding: 11px 20px !important;
border-radius: 4px !important;
color: #fff !important;
text-align: center !important;
font-weight: bold !important;
vertical-align: top !important;
cursor: pointer !important;
-webkit-transition: all .2s ease-out !important;
transition: all .2s ease-out !important;

font-size: 15px !important;
font-weight: bold !important;
width: auto !important;
float: right;
outline: none;
margin-right: 25px
}

.form_wrapper select{
	color: #08b4e0  !important
}
.form_wrapper input:focus, .form_wrapper select:focus{
	outline: none;
	border-color: inherit !important;
}

.flickers_images .close{
  opacity: 1;
  font-size: 14px;
  text-align: center;
  line-height: 25px;
  text-decoration: none;

}
.flickers_images .close i{
	display: block;
	text-align: center;
	line-height: 20px;
	font-weight: normal;
	font-size:14px;
}
.flickers_images .flicker_imagedata li {
	border: 3px solid #fff;
	padding: 0;
	margin: 2px;
	height: 50px;
	border-radius: 5px;
	border: 3px solid #fff
}

.flickers_images .flicker_imagedata .select{
	border-radius: 4px;
	border-color: #08b4e0;
    box-shadow: 0 0 6px;
    position: relative;
}

.flickers_images .flicker_imagedata li img{
	display: block;
	height: 100%
}
.flickers_images .flicker_imagedata .select:before{
	content: '';
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,.8);
	position: absolute;
}
.flickers_images .flicker_imagedata .select:after{
	content: '';
	background: url(http://beta.canvassignages.com/skin/frontend/rwd/default/images/canvas_live_editor/images/select-check.png) no-repeat;
	position: absolute;
	right: 5px;
	top: 5px;
	width: 20px;
	height: 20px;
	z-index: 999999;
}

.flickimage_upload {
	background: #08b4e0;
color: #fff;
box-shadow: transparent 0 0 0 inset;
transition: ease-in-out all 0.4s;
-webkit-transition: ease-in-out all 0.4s;
-ms-transition: ease-in-out all 0.4s;
-o-transition: ease-in-out all 0.4s;
padding: 10px 20px;
border-radius: 3px;
color: #4c4c4c;
font-family: Lato;
font-size: 13px;
font-weight: 900;
padding: 15px 30px;
text-decoration: none;
text-transform: uppercase;
display: block;
color: #fff;
margin:0 auto;
}
.flickimage_upload:focus{
	outline: none;
}

