#textEditorArea{

    width: 512px;

    min-width: 512px;

    height: auto;

     box-shadow: 2px 2px 8px -2px #888;

    border-top: 1px solid #c8cbcc;

    border-left: 1px solid #c8cbcc;

    border-right: 1px solid #c8cbcc;

    border-bottom: 1px solid #c8cbcc;

    border: 1px solid #c8cbcc;

    background-color: #fff;

    position: absolute;

    z-index: 999;

    right: 0px;

   // transform: translateX(-50%);

    top: 50%;

    left: 100px;

   margin: auto;

    display: none;

}

.editroPlace{

        width: 96%;

    margin: 8px 2%;

    max-height: 65px;

    overflow: auto;

        padding: 10px 13px;

    background-color: #f3f3f3;    

}

.editedText{

    border:none;

    background: none;

    outline: none;

    width: 100%;

}



.editorOptions ul{

    margin: 0;

    padding: 0;

    list-style: none;

    background: #fff;

    position: relative;

    z-index: 9;

    background: -webkit-linear-gradient(top,#ffffff,#f9f9f9);

    background: linear-gradient(to bottom,#ffffff,#f9f9f9);

    overflow: hidden;

    width: 900px;

    transition: all .3s ease;

    -webkit-transition: all .3s ease;

    -moz-transition: all .3s ease;

    -ms-transition: all .3s ease;

}

.editorOptions{

    width: 100%;

    height: auto;

    border-bottom: 1px solid #c8cbcc;

    position: relative;

    

}

.editorOptions ul li{

    display: inline-block;

    vertical-align: top;

    cursor: pointer;

}

.editorOptions ul li:not(:first-child){

    margin-left: -4px;

}

.editorOptions ul li.fontsArea{

    width: 242px;

    height: auto;

    border-right: 1px solid #c8cbcc;

    position: relative;

}

.fontName{

    width: 165px;

    height: 30px;

    float: left;

    background-color: white;

    margin-top: 10px;

    margin-left: 10px;

    margin-right: 10px;

    margin-bottom: 10px;

    border-radius: 20px;

    border-top: 1px solid #c8cbcc;

    border-left: 1px solid #c8cbcc;

    border-right: 1px solid #c8cbcc;

    border-bottom: 1px solid #c8cbcc;

    border: 1px solid #c8cbcc;

    overflow: hidden;

    cursor: pointer;

    text-align: center;

    box-shadow: 0 3px 8px -5px #000 inset;

}

.font-family-display-name {

   // background: url(http://www.vistaprint.in/any/studio/fontsprite.caspx?mode=Default&languageid=In&hexvalue=000000);

    width: 250px;

    height: auto;

    background-position: 13px -200px;

    margin-top: 2px;

    padding: 0px 15px;

    text-align: left;

}

.fontSize{

    float: left;

    width: 45px;

    height: 30px;

    background-color: white;

    margin-top: 10px;

    margin-right: 10px;

    border-radius: 20px;

    border-top: 1px solid #c8cbcc;

    border-left: 1px solid #c8cbcc;

    border-right: 1px solid #c8cbcc;

    border-bottom: 1px solid #c8cbcc;

    border: 1px solid #c8cbcc;

