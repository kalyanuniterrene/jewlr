body{
    overflow: hidden;
}

.canvasWrap {
    box-shadow: 0 0 6px rgba(0, 0, 0, 0.4);
}
.abs_canvas {
    position: absolute;
    background: rgba(0, 0, 0, .1);
    z-index: 9;
    transition: all .3s ease;
    -webkit-transition: all .3s ease;
    -moz-transition: all .3s ease;

}

#topCanvas {
    border-bottom: 2px dashed #08b4e0;
}

#rightCanvas {

    border-left: 2px dashed #08b4e0;

}



#bottomCanvas {

    border-top: 2px dashed #08b4e0;

}



#leftCanvas {

    border-right: 2px dashed #08b4e0;

}



.loading_img {
	text-align:center;
    position: absolute;
    z-index: 10;
    left: 0;
    right: 0;
    margin: auto;
    top: 0%;
    bottom: 0;
    background: #fff;
    width: 100%;
}


.loading_img img {

    max-width: 200px !important;

    height: auto !important;

    box-shadow: none !important;

    position: relative;

    top: 50%;

    transform: translateY(-50%);

}



.disable{

	cursor: no-drop;

}

.ui-draggable-dragging{

	width: 150px !important;

	height: auto;

	cursor: move;

}

.low_resolution_modal{

	background: rgba(0,0,0, .5);

	display: none;	

	z-index: 9999999;

}

.low_resolution_modal > .low-res-modal{

	background-color: #f8f8f8;

    box-shadow: 0 8px 10px rgba(0,0,0,0.25);

    -webkit-border-radius: 3px;

    -moz-border-radius: 3px;

    -ms-border-radius: 3px;

    -o-border-radius: 3px;

    border-radius: 3px;

    margin: auto;

    width: 480px;

    position: relative;

    top: 50%;

    transform: translateY(-50%);

}

.low-res-modal{

	text-align: center;

	padding: 20px;

}

.close_low_res_popup{

	background: #363636 none repeat scroll 0 0;

    border-radius: 3px;

    color: #fff;

    font-family: Lato;

    font-size: 13px;

    font-weight: normal;

    line-height: normal;

    opacity: 1;

    padding: 5px 8px;

    position: absolute;

    right: 0;

    text-transform: uppercase;

    top: -35px;

}

.close_low_res_popup img{

	margin-left: 7px;

    margin-top: -3px;

}


.color_picker_box .close_low_res_popup {
    background: transparent;
    border-radius: 3px;
    color: #fff;
    font-family: Lato;
    font-size: 13px;
    font-weight: normal;
    line-height: normal;
    opacity: 1;
    padding: 0;
    position: initial; 
    right: 0;
    text-transform: uppercase;
    top: inherit;
    display: inline-block;
    padding-right: 15px;
}
.color_picker_box input{
	display: inline-block;
    max-width: 60px;
}
.color_picker_box .close_low_res_popup img {
display: none;
}

.color{

	border-color: #fff;

}

.uploaded-photo-box{

	cursor: pointer;

}



.layerBox{

    width: 250px !important;

    min-height: 80px;

    position: absolute;

    top: 50%;

    background: #fff;

    border:5px solid rgba(0,0,0, .5);

    right: 10%;

    bottom: 0%;

    z-index: 9999999999;

    transition: all .3s ease;

    -webkit-transition: all .3s ease;

    -moz-transition: all .3s ease;

}

.layerBox h4{

    background: #4c4c4c;

    margin: 0;

    padding: 10px;

    text-align: center;

    color: #fff;

    font-size: 14px;

}

.layerItems{

    list-style-type: none;

    margin:0;

    padding: 0;

}

.layerItems li{

    background: #f7f7f7;

    margin-bottom: 2px;

    margin-top: 2px;

    padding: 10px 10px 10px 60px;

    border: 2px solid #ddd;

    cursor: pointer;

    position: relative;

}

.layerItems li span{

    font-family: Open Sans;

}

.layerItems li img{

    width: 50px;

    position: absolute;

    left: 0;

    top: 0;

    bottom: 0;

    height: 100%;

}

.layerItem.active{

    border:2px solid #f00;

}

.layerItem i{

    position: absolute;

    right: 0;

    top: 0;

    bottom: 0;

    width: 35px;

    text-align: center;

    padding-top: 11px;

}

.layerHidden{

    top: 100% !important;

}

.color_picker_box{

    position: absolute;
    right: 20%;
    bottom: 30%;
    background: rgba(0,0,0,.8);  
    display: none;
    z-index: 999999;
    padding: 8px 15px;

}



.resetColor{

    background: #f4f4f4 !important;

}

.resetColor_new{

    background: rgba(0, 0, 0, .2) !important;

}

.minimize_color{

    position: fixed;   

    top: 100.7%;

    right: 20%;

}

.hide_canvas{

    display: none;

}

.sidebar-content.style .tab-content li{

    display: inline-block;

    vertical-align: top;

    float: inherit;

    width: 49%;

}

.inputSets{

    display: none;

}



.textEditorSec{

    display: none;

    width: 500px !important;

    height: auto;

    position: absolute;

    top: 20%;

    transform: translateY(-50%);

    left: 50%;

    transform: translateX(-50%);

    background: -webkit-linear-gradient(top, rgba(255,0,0,0), rgba(255,0,0,1));

    background: #fff;

    z-index: 999;

    border:2px solid #c9cacc;

}

.textEditorSec .editor-heading{

    width: 100%;

    height: 28px;

    background: -webkit-linear-gradient(top, rgba(255,255,255,0), #ddd);

    border-bottom:2px solid #c9cacc;

}

.editor-body{

    width: 100%;

    height: 180px;    

    background: red;

}

.editor-body .leftPanel{

    width: 120px;

    height: 100%;

    overflow-y: auto;

    float: left;

}

.fontLists{

    width: 100%;

    height: 300px;

    background: #fff;

}

.rightPanel{

    width: 376px;

    height: 100%;

    background: #f7f7f7;

    float: left;

    border-left: 1px solid #ddd;

    overflow-y: auto;

}

.shortedFontList{

    width: 100%;

    height: 300px;

}

.editor-footer{

    width: 100%;

    height: 200px;

    border-top: 2px solid #ddd;

}



.d_view_wraper{

    display: none;

    position: absolute;

    top: 0;

    left: 0;

    right: 0;

    width: 440px;

    height: 440px;

    margin: 0 auto;

    background: transparent;    

    transform-style: preserve-3d;

    transform: rotateX(0deg) rotateY(0deg) scale(0.8,0.8);

    transition: all .3s ease;

    -webkit-transition: all 1s ease;

    -moz-transition: all 1s ease;

    -moz-transition: all 1s ease;

    -ms-transition: all 1s ease;

    -o-transition: all 1s ease;

}

.d_sides{

    width: 100%;

    height: 100%;

    background: #000;

    display: block;

    position: absolute;

    transform-origin: 0 0;

    -webkit-transition: all 1s ease;

    -moz-transition: all 1s ease;

    -moz-transition: all 1s ease;

    -ms-transition: all 1s ease;

    -o-transition: all 1s ease;

}
.d_sides img{
    display: block;
}

.d_sides.front{

    background: #fff;

    border:2px dashed rgb(8, 180, 224);

}

.hideCanvas{

    display: none !important;

}

.show_d_box{

    display: block;

}

.changeColor{

    background: #4d4d4d !important;

    box-shadow: none !important;

}



